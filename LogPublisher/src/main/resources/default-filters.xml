<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<filters>
	<category name="exclude">
		<filter name="ScalaTestRunFailure">
			<cause
				keyword="org.scalatest:scalatest-maven-plugin" />
			<cause keyword="There are test failures" />
		</filter>
	
		<filter name="TestRunFailure">
			<cause
				keyword="Failed to execute goal org.apache.maven.plugins:maven-surefire-plugin" />
			<cause keyword="There are test failures" />
		</filter>
		<filter name="CompilationFailure">
			<cause
				keyword="Failed to execute goal org.apache.maven.plugins:maven-compiler-plugin" />
		</filter>
		<filter name="NoTestSuiteFile">
			<cause
				keyword="Failed to execute goal org.apache.maven.plugins:maven-surefire-plugin" />
			<cause keyword="TestSetFailedException: Suite file" />
		</filter>
		<filter name="NoTestRunFailure">
			<cause
				keyword="Failed to execute goal org.apache.maven.plugins:maven-surefire-plugin" />
			<cause keyword="No tests were executed" />
		</filter>
		<filter name="TestExitInIllegalState">
			<cause keyword="org.apache.maven.plugins:maven-surefire-plugin"/>
			<cause keyword="java.lang.IllegalStateException: Shutdown in progress"/>
		</filter>		
		
		<filter name="LSPCodeGenFail">
			<cause keyword="com.ebay.lsp.build:code-gen-plugin"/>
		</filter>
		<filter name="TestClassMissing">
			<cause keyword="org.apache.maven.plugins:maven-surefire-plugin"/>
			<cause keyword="A required class was missing while executing"/>
		</filter>
		<filter name="ContentPluginFail">
			<cause keyword="com.ebay.content:maven-content-plugin"/>
		</filter>
		<filter name="ErrorForkTest">
			<cause keyword="org.apache.maven.plugins:maven-surefire-plugin"/>
			<cause keyword="Error while executing forked tests"/>
		</filter>
		<filter name="EnunciateFail">
			<cause keyword="org.codehaus.enunciate:maven-enunciate-plugin"/>
		</filter>
		<filter name="SiteExecutionFailure">
			<cause
				keyword="Failed to execute goal org.apache.maven.plugins:maven-site-plugin" />
		</filter>
		<filter name="FindbugsPluginFail">
			<cause keyword="Failed to execute goal org.codehaus.mojo:findbugs-maven-plugin"/>
		</filter>
